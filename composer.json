{
  "name": "swaggest/swagger2-schema",
  "description": "Swagger 2.0 schema PHP mappings",
  "type": "library",
  "require": {
    "swaggest/json-schema": "^0.12.16",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "swaggest/php-code-builder": "^0.2.20"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Viacheslav Poturaev",
      "email": "vearutop@gmail.com"
    }
  ],
  "support": {
    "email": "vearutop@gmail.com"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Swaggest\\SwaggerHttp\\": "src/Http/",
      "Swaggest\\SwaggerSchema\\": "src/SwaggerSchema/",
      "Swaggest\\OpenAPI3Schema\\": "src/OpenAPI3Schema/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Swaggest\\SwaggerSchema\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "platform": {
      "php": "5.6.0"
    }
  }
}
