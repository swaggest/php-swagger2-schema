{
    "name": "swaggest/swagger2-schema",
    "description": "Swagger 2.0 schema PHP mappings",
    "type": "library",
    "require": {
        "swaggest/json-schema": "dev-export-schema|*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.35",
        "swaggest/code-builder": "dev-wip-client-2",
        "swaggest/php-code-builder": "dev-wip-client-2",
        "phpunit/php-code-coverage": "2.2.4",
        "codeclimate/php-test-reporter": "^0.4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Poturaev",
            "email": "vearutop@gmail.com"
        }
    ],
    "support": {
        "email": "vearutop@gmail.com"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Swaggest\\SwaggerSchema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swaggest\\SwaggerSchema\\Tests\\": "tests/src/"
        }
    },
    "scripts": {
        "gen-swg-struct": "php ./tools/generate_swagger_structures.php"
    },
    "config": {
        "platform": {
            "php": "5.4.45"
        }
    }
}
